<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.wordpress.ambergautam.fbclone">

    <application
        android:allowBackup="true"
        android:icon="@drawable/fcb"
        android:label="@string/app_name"
        android:roundIcon="@drawable/fcb"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Log_in"
            android:screenOrientation="portrait">

            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Create_new_account"
            android:label="@string/app_nam"/>
        <activity android:name=".Name"
            android:label="@string/app_namm"/>
        <activity android:name=".Mobile_no"
            android:label="@string/app_mob"/>
        <activity android:name=".Birthday"
            android:label="@string/app_bday"/>
        <activity android:name=".Gender"
            android:label="@string/app_gender"/>
        <activity android:name="Password"
            android:label="@string/app_pass"/>
        <activity android:name=".Terms_Policy"
            android:label="@string/app_policy"></activity>
    </application>

</manifest>